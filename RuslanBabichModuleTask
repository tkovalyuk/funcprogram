#lang racket

;Визначити процедуру СУМА_ДОБУТОК(Х), де Х - список довільної довжини,
;який складається з цілих чисел, а результат процедури – список
;двох елементів: сума та добуток цих чисел.

(define (task L)(
   if (check L) 
      (list (apply + L) (apply * L))
   '()
))

(define (check L)
  (and (not (null? L))(list? L))
)

(task 1)
(task '(1 2 3 4))
